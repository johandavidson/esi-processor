{
  "name": "esi-processor",
  "version": "0.0.1",
  "description": "A processor for processing ESI tags in html documents. Currently supporting esi:include, esi:remove, esi:comment, <!--esi --> and esi:choose.",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "test": "jest",
    "tsc": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript\" -c \"yellow.bold\" \"npm run watch-ts\"",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johandavidson/esi-processor.git"
  },
  "keywords": [
    "esi"
  ],
  "author": "Johan Davidsson <johan.davidsson@trewit.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johandavidson/esi-processor/issues"
  },
  "homepage": "https://github.com/johandavidson/esi-processor#readme",
  "dependencies": {
    "domutils": "^1.7.0",
    "htmlparser2": "^3.10.1",
    "node-html-parser": "^1.1.12",
    "request-promise-native": "^1.0.7",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/domelementtype": "^1.3.0",
    "@types/domhandler": "^2.4.0",
    "@types/domutils": "^1.7.1",
    "@types/htmlparser2": "^3.7.31",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/request-promise-native": "^1.0.15",
    "@types/xml-js": "^1.0.0",
    "concurrently": "^4.1.0",
    "jest": "^24.0.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  }
}
