{
  "name": "esi-processor",
  "version": "0.1.3",
  "description": "A processor for processing ESI tags in html documents. Currently supporting esi:include, esi:remove, esi:comment, <!--esi -->, esi:choose and esi:vars.",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "test": "jest --coverage",
    "tsc": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "preversion": "npm test && npm run build",
    "postversion": "git push origin v$npm_package_version && echo \"Successfully released version $npm_package_version!\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript\" -c \"yellow.bold\" \"npm run watch-ts\"",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johandavidson/esi-processor.git"
  },
  "keywords": [
    "esi"
  ],
  "author": "Johan Davidsson <johan.davidsson@trewit.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johandavidson/esi-processor/issues"
  },
  "homepage": "https://github.com/johandavidson/esi-processor#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "domutils": "^1.7.0",
    "htmlparser2": "^3.10.1",
    "node-html-parser": "^1.1.16",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/domelementtype": "^1.3.0",
    "@types/domhandler": "^2.4.1",
    "@types/domutils": "^1.7.2",
    "@types/express": "^4.17.2",
    "@types/htmlparser2": "^3.10.1",
    "@types/jest": "^24.0.22",
    "@types/node": "^11.15.2",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "node-mocks-http": "^1.8.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
